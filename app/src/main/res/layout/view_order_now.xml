<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp"
    android:orientation="vertical">
    <android.support.v7.widget.CardView
        android:id="@+id/cardview_order_now"
        app:cardBackgroundColor="@color/lightgray"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <ImageView
            android:id="@+id/icon_restaurant_now"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/abc_ic_star_black_36dp" />
        <TextView
            android:id="@+id/textView_restaurant_name_now"
            android:text="@string/restaurant_name"
            android:layout_marginLeft="45dp"
            android:gravity="center"
            android:textSize="23dp"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="35dp" />
        <TextView
            android:id="@+id/textView_order_completed_now"
            android:text="@string/order_doing"
            android:layout_gravity="right|top"
            android:layout_marginRight="15dp"
            android:gravity="center"
            android:textSize="18dp"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="35dp" />
        <LinearLayout
            android:layout_gravity="top"
            android:layout_marginTop="38dp"
            android:background="@color/blackdefine"
            android:layout_width="match_parent"
            android:layout_height="0.4dp"/>
        <FrameLayout
            android:orientation="horizontal"
            android:layout_gravity="top"
            android:layout_marginTop="38.2dp"
            android:layout_width="match_parent"
            android:layout_height="35dp">
            <TextView
                android:id="@+id/textView_seat_now"
                android:text="@string/seat"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_gravity="left"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_seat_count_now"
                android:text="@string/seat_count"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="right"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <LinearLayout
                android:layout_gravity="bottom|left"
                android:layout_marginLeft="55dp"
                android:background="@color/blackdefine"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
        </FrameLayout>
        <FrameLayout
            android:orientation="horizontal"
            android:layout_gravity="top"
            android:layout_marginTop="73.2dp"
            android:layout_width="match_parent"
            android:layout_height="35dp">
            <TextView
                android:id="@+id/textView_date_now"
                android:text="@string/date"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_gravity="left"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_date_details_now"
                android:text="@string/date_details"
                android:textSize="12dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="right"
                android:gravity="center_vertical|right"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <LinearLayout
                android:layout_gravity="bottom|left"
                android:layout_marginLeft="55dp"
                android:background="@color/blackdefine"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
        </FrameLayout>
        <FrameLayout
            android:layout_gravity="top"
            android:layout_marginTop="108.2dp"
            android:layout_width="match_parent"
            android:layout_height="35dp">
            <TextView
                android:id="@+id/textView_people_now"
                android:text="@string/people"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_gravity="left"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_people_count_now"
                android:text="@string/people_count"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="right"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <LinearLayout
                android:layout_gravity="bottom|left"
                android:layout_marginLeft="55dp"
                android:background="@color/blackdefine"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
        </FrameLayout>
        <FrameLayout
            android:layout_marginTop="143.5dp"
            android:layout_gravity="top"
            android:layout_width="match_parent"
            android:layout_height="140dp">
            <TextView
                android:id="@+id/textView_food_name_now"
                android:text="@string/food_name"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_gravity="left"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_food_name_first_now"
                android:text="@string/food_name_first"
                android:textSize="15dp"
                android:gravity="center"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="35dp"
                android:layout_gravity="top|left"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_food_name_first_count_now"
                android:text="@string/food_name_first_count"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="top|right"
                android:layout_marginTop="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_food_name_more"
                android:text="@string/food_name_more"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_marginLeft="55dp"
                android:layout_gravity="bottom|left"
                android:layout_marginBottom="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_details_now"
                android:text="@string/details"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="bottom|right"
                android:layout_marginBottom="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <LinearLayout
                android:background="@color/blackdefine"
                android:layout_gravity="bottom"
                android:layout_marginBottom="35dp"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
            <TextView
                android:id="@+id/textView_not_review_now"
                android:text="@string/price2"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:padding="2dp"
                android:gravity="center"
                android:layout_marginRight="17dp"
                android:layout_gravity="bottom|right"
                android:layout_marginBottom="5dp"
                android:layout_width="wrap_content"
                android:layout_height="25dp" />
        </FrameLayout>
        <TextView
            android:text="@string/order_status"
            android:textColor="@color/black"
            android:background="@color/white"
            android:textSize="18dp"
            android:gravity="center"
            android:layout_gravity="top"
            android:layout_marginTop="283.2dp"
            android:layout_width="match_parent"
            android:layout_height="35dp" />
        <LinearLayout
            android:layout_gravity="top"
            android:layout_marginTop="283.2dp"
            android:layout_width="match_parent"
            android:layout_height="15dp"/>
        <FrameLayout
            android:id="@+id/framelayout_waiting_pay"
            android:visibility="gone"
            android:layout_gravity="top"
            android:layout_marginTop="333.2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/icon_order_status_steps1"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:background="@drawable/abc_seekbar_thumb_material" />
            <TextView
                android:id="@+id/textview_waiting_pay"
                android:text="@string/waiting_pay"
                android:gravity="center"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:layout_gravity="left"
                android:layout_marginLeft="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textview_remaining_time"
                android:text="@string/remaining_time"
                android:textColor="@color/black"
                android:layout_marginTop="35dp"
                android:layout_gravity="center_horizontal|top"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textview_cancel"
                android:text="@string/cancel"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:background="@drawable/shape_corner_rectangle_radian"
                android:gravity="center"
                android:layout_gravity="top|left"
                android:layout_marginTop="75dp"
                android:layout_marginLeft="50dp"
                android:layout_width="100dp"
                android:layout_height="25dp" />
            <TextView
                android:id="@+id/textview_go_paying"
                android:text="@string/go_paying"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:background="@drawable/shape_corner_rectangle_radian"
                android:gravity="center"
                android:layout_gravity="top|right"
                android:layout_marginTop="75dp"
                android:layout_marginRight="50dp"
                android:layout_width="100dp"
                android:layout_height="25dp" />
        </FrameLayout>
        <FrameLayout
            android:id="@+id/framelayout_payed"
            android:visibility="visible"
            android:layout_gravity="top"
            android:layout_marginTop="333.2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/icon_order_status_steps2"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:background="@drawable/abc_seekbar_thumb_material" />
            <TextView
                android:id="@+id/textview_payed"
                android:text="@string/payed"
                android:gravity="center"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:layout_gravity="left"
                android:layout_marginLeft="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textview_time_payed"
                android:text="@string/time_payed"
                android:gravity="center"
                android:textSize="12dp"
                android:layout_gravity="right"
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
        </FrameLayout>
        <FrameLayout
            android:id="@+id/framelayout_conform"
            android:layout_gravity="top"
            android:layout_marginTop="368.2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/icon_order_status_steps3"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:background="@drawable/abc_seekbar_thumb_material" />
            <TextView
                android:id="@+id/textview_conform"
                android:text="@string/conform"
                android:gravity="center"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:layout_gravity="left"
                android:layout_marginLeft="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textview_time_conform"
                android:text="@string/time_conform"
                android:gravity="center"
                android:textSize="12dp"
                android:layout_gravity="right"
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
        </FrameLayout>
        <TextView
            android:visibility="visible"
            android:id="@+id/textview_warn"
            android:text="@string/warn"
            android:textColor="@color/black"
            android:background="@color/white"
            android:textSize="18dp"
            android:gravity="center"
            android:layout_gravity="top"
            android:layout_marginTop="403.2dp"
            android:layout_width="match_parent"
            android:layout_height="35dp" />
        <TextView
            android:visibility="visible"
            android:id="@+id/textview_warn_details"
            android:text="@string/warn_details"
            android:gravity="left"
            android:layout_gravity="top"
            android:layout_marginTop="438.2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </android.support.v7.widget.CardView>


</LinearLayout>