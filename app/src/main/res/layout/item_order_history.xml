<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp"
    android:orientation="vertical">
    <android.support.v7.widget.CardView
        android:id="@+id/cardview_order_history"
        app:cardBackgroundColor="@color/lightgray"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <ImageView
            android:id="@+id/icon_restaurant"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/abc_ic_star_black_36dp" />
       <TextView
           android:id="@+id/textView_restaurant_name"
           android:text="@string/restaurant_name"
           android:layout_marginLeft="45dp"
           android:gravity="center"
           android:textSize="23dp"
           android:textColor="@color/black"
           android:layout_width="wrap_content"
           android:layout_height="35dp" />
        <TextView
            android:id="@+id/textView_order_completed"
            android:text="@string/order_completed"
            android:layout_gravity="right|top"
            android:layout_marginRight="15dp"
            android:gravity="center"
            android:textSize="18dp"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="35dp" />
        <LinearLayout
            android:layout_gravity="top"
            android:layout_marginTop="38dp"
            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="0.4dp"/>
        <FrameLayout
            android:orientation="horizontal"
            android:layout_gravity="top"
            android:layout_marginTop="38.2dp"
            android:layout_width="match_parent"
            android:layout_height="35dp">
            <TextView
                android:id="@+id/textView_seat"
                android:text="@string/seat"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_gravity="left"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_seat_count"
                android:text="@string/seat_count"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="right"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <LinearLayout
                android:layout_gravity="bottom|left"
                android:layout_marginLeft="55dp"
                android:background="@color/black"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
        </FrameLayout>
        <FrameLayout
            android:layout_gravity="top"
            android:layout_marginTop="73.2dp"
            android:layout_width="match_parent"
            android:layout_height="35dp">
            <TextView
                android:id="@+id/textView_people"
                android:text="@string/people"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_gravity="left"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_people_count"
                android:text="@string/people_count"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="right"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <LinearLayout
                android:layout_gravity="bottom|left"
                android:layout_marginLeft="55dp"
                android:background="@color/black"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
        </FrameLayout>
        <FrameLayout
            android:layout_marginTop="108.2dp"
            android:layout_gravity="top"
            android:layout_width="match_parent"
            android:layout_height="140dp">
            <TextView
                android:id="@+id/textView_food_name"
                android:text="@string/food_name"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_gravity="left"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_food_name_first"
                android:text="@string/food_name_first"
                android:textSize="15dp"
                android:gravity="center"
                android:textColor="@color/black"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="35dp"
                android:layout_gravity="top|left"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_food_name_first_count"
                android:text="@string/food_name_first_count"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="top|right"
                android:layout_marginTop="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_food_name_more"
                android:text="@string/food_name_more"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_marginLeft="55dp"
                android:layout_gravity="bottom|left"
                android:layout_marginBottom="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_details"
                android:text="@string/details"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:layout_marginRight="15dp"
                android:layout_gravity="bottom|right"
                android:layout_marginBottom="35dp"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <LinearLayout
                android:background="@color/black"
                android:layout_gravity="bottom"
                android:layout_marginBottom="35dp"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
            <TextView
                android:id="@+id/textView_price"
                android:text="@string/price1"
                android:textSize="15dp"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_marginLeft="35dp"
                android:layout_gravity="bottom|left"
                android:layout_width="wrap_content"
                android:layout_height="35dp" />
            <TextView
                android:id="@+id/textView_not_review"
                android:text="@string/not_review"
                android:background="@drawable/shape_corner_rectangle_radian"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:padding="2dp"
                android:gravity="center"
                android:layout_marginRight="17dp"
                android:layout_gravity="bottom|right"
                android:layout_marginBottom="5dp"
                android:layout_width="wrap_content"
                android:layout_height="25dp" />
        </FrameLayout>
        </android.support.v7.widget.CardView>


    </LinearLayout>